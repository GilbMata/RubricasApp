{
  "name": "nodeapi",
  "version": "0.0.1",
  "author": "Lukas Aichbauer <l.aichbauer@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "npm run nodemon",
    "dev": "cross-env NODE_ENV=development node ./api/api.js",
    "nodemon": "nodemon --exec npm run dev",
    "lint": "eslint ./api/. ./config/. ./test/.",
    "prepush": "npm test",
    "pretest": "npm run lint",
    "production": "cross-env NODE_ENV=production node ./api/api.js",
    "test": "cross-env NODE_ENV=testing jest --coverage;",
    "test-ci": "jest --coverage --forceExit"
  },
  "jest": {
    "globals": {
      "__DEV__": true
    },
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.20.2",
    "connect-history-api-fallback": "^2.0.0",
    "core-js": "^2.6.9",
    "cors": "^2.8.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-routes-mapper": "^1.1.0",
    "formidable": "^2.1.1",
    "helmet": "^3.12.0",
    "jsonwebtoken": "^8.2.1",
    "mariadb": "^3.1.0",
    "morgan": "^1.10.0",
    "sequelize": "^6.29.3"
  },
  "devDependencies": {
    "cross-env": "^5.1.4",
    "husky": "^0.14.3",
    "jest": "^24.8.0",
    "nodemon": "^1.17.3",
    "supertest": "^3.0.0"
  }
}
